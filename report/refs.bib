	urldate = {2024-07-02},
	date = {2016-12},
	keywords = {3D virtual worlds, Algorithm design and analysis, algorithm visualizations, Artificial intelligence, Avatars, Education, learning efficiency, teaching search algorithms, Three-dimensional displays, virtual reality, Visualization},
	file = {IEEE Xplore Abstract Record:/Users/totoro/Zotero/storage/QMLGVTVA/7814804.html:text/html;IEEE Xplore Full Text PDF:/Users/totoro/Zotero/storage/CJ69HH4G/Grivokostopoulou et al. - 2016 - An Innovative Educational Environment Based on Vir.pdf:application/pdf},
